{
  "apps": [
    {
      "name": "journey-server",
      "script": "./dist/main.js",
      "exec_mode": "cluster",
      "instances": 1,
      "instance_var": "INSTANCE_ID",
      "autorestart": true,
      "watch": false,
      "max_memory_restart": "500M",
      "env_dev": {
        "NODE_ENV": "development"
      },
      "env_prd": {
        "NODE_ENV": "production"
      }
    }
  ]
}
